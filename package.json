{
  "name": "mqtt-level-store",
  "version": "1.0.0",
  "description": "Store your in-flight mqtt packets in Level",
  "main": "mqtt-level-store.js",
  "scripts": {
    "test": "mocha test.js",
    "pretest": "npm run check-style",
    "check-style:eslint": "eslint *.js",
    "check-style:jshint": "jshint *.js",
    "check-style:jscs": "jscs *.js",
    "check-style": "npm run check-style:jshint && npm run check-style:jscs && npm run check-style:eslint"
  },
  "pre-commit": "test",
  "keywords": [
    "mqtt",
    "level",
    "levelup",
    "store"
  ],
  "author": "Matteo Collina <matteo.collina@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
